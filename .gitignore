# Ignore directories generated by Composer
drush/contrib
vendor
bin
docroot/core
docroot/libraries
docroot/modules/contrib
docroot/themes/contrib
docroot/profiles/contrib

# Ignore files subdirectories
docroot/sites/default/files/css
docroot/sites/default/files/styles
docroot/sites/default/files/js
docroot/sites/default/files/php

private/*
tmp/*

# Ignore files generated by PhpStorm
.idea

# Ignore apple stuff
.DS_Store

